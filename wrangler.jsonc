{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "caos-trail",
  "build": { "command": "yarn run build", "cwd": "." },
  "pages_build_output_dir": "./dist",
  "observability": {
    "enabled": false,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "persist": true,
      "invocation_logs": true
    },
    "traces": {
      "enabled": false,
      "persist": true,
      "head_sampling_rate": 1
    }
  }
}